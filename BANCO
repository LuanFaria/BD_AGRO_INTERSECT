CREATE TABLE public.base_intersect_ndvi (
    id SERIAL PRIMARY KEY,
    clientes_id INTEGER NOT NULL REFERENCES public.clientes(id),
    gridcode INTEGER,
    setor VARCHAR(50),
    fazenda VARCHAR(100),
    bloco VARCHAR(100),
    talhao VARCHAR(100),
    safra INTEGER,
    id_original VARCHAR(100),
    tp_prop VARCHAR(50),
    chave VARCHAR(200),
    objetivo VARCHAR(100),
    secao VARCHAR(100),
    pivo VARCHAR(50),
    desc_faz VARCHAR(200),
    variedade VARCHAR(100),
    maturacao VARCHAR(50),
    ambiente VARCHAR(50),
    estagio VARCHAR(100),
    grupo_dash VARCHAR(100),
    grupo_ndvi VARCHAR(100),
    nmro_corte VARCHAR(50),
    desc_cana VARCHAR(200),
    area_bd DOUBLE PRECISION,
    a_est_moag DOUBLE PRECISION,
    a_colhida DOUBLE PRECISION,
    a_est_muda DOUBLE PRECISION,
    a_muda DOUBLE PRECISION,
    tch_est DOUBLE PRECISION,
    tc_est DOUBLE PRECISION,
    tch_rest DOUBLE PRECISION,
    tc_rest DOUBLE PRECISION,
    tch_real DOUBLE PRECISION,
    tc_real DOUBLE PRECISION,
    dt_corte DATE,
    dt_ult_cor DATE,
    dt_plantio DATE,
    idade_cort DOUBLE PRECISION,
    atr DOUBLE PRECISION,
    atr_est DOUBLE PRECISION,
    irrigacao VARCHAR(50),
    tah DOUBLE PRECISION,
    tph DOUBLE PRECISION,
    cliente VARCHAR(100),
    tch_v0 DOUBLE PRECISION,
    tc_v0 DOUBLE PRECISION,
    area_gis DOUBLE PRECISION,
    obs_img VARCHAR(255),
    data_img DATE,
    idade_img DOUBLE PRECISION,
    classe VARCHAR(100),
    area_ndvi DOUBLE PRECISION,
    janela VARCHAR(10),
    geom geometry(MULTIPOLYGON, 4326)
);


